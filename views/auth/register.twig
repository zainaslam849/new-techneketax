<script src="https://unpkg.com/notie"></script>
<script>
    // $(document).ready(function() {
    //     document.getElementById("cover-spin").classList.add('hide');
    // });
    // Get the input field
    var input = document.getElementById("password");
    // Execute a function when the user presses a key on the keyboard
    input.addEventListener("keypress", function(event) {
        // If the user presses the "Enter" key on the keyboard
        if (event.key === "Enter") {
            // Cancel the default action, if needed
            event.preventDefault();
            // Trigger the button element with a click
            document.getElementById("btn").click();
        }
    });
    var myInput = document.getElementById("password");
    // When the user clicks on the password field, show the message box
    // LOGIN AND SIGNUP JS
    function do_register() {
        document.getElementById("btn").disabled = true;
        document.getElementById("btn").innerHTML = "Loading";
        var firm_id = $("#firm_id").val();
        var invite = $("#invite").val();
        var fname = $("#fname").val();
        var lname = $("#lname").val();
        var email = $("#email").val();
        var phone = $("#phone").val();
        var pass = $("#password").val();
        var cpassword = $("#cpassword").val();
        var account_type = $("input[name=account_type]:checked").val();
        var csrf_token = $("input[name=csrf]").val();

        if (fname != "" && lname != "" && email != "" && phone != "" && pass != "" && cpassword != ""&& account_type != "") {
            if (pass == cpassword) {
                if (firm_id != "" && invite != ""){
                    $.ajax
                    ({
                        type: 'post',
                        url: '/join/$firm_id/$invite',
                        data: {
                            do_register: "do_register",
                            firm_id: firm_id,
                            invite: invite,
                            fname: fname,
                            lname: lname,
                            email: email,
                            phone: phone,
                            password: pass,
                            table_name: 'users',
                            type: 'user',
                            csrf: csrf_token,
                        },
                        success: function (response) {
                            response = $.parseJSON(response);
                            document.getElementById("btn").innerHTML = "Register";
                            document.getElementById("btn").disabled = false;

                            if (response == "1") {
                                notie.alert({type: 'success', text: 'Registered Successfully', time: 2})
                                location.href='/login'
                            } else if(response == "2") {
                                notie.alert({type: 'error', text: 'Email Already Exist. Login to continue.', time: 2})
                            } else if(response == "3") {
                                notie.alert({type: 'error', text: 'A minimum 8 characters password contains a combination of uppercase and lowercase letter and number.', time: 2})
                            } else if(response == "4") {
                                notie.alert({type: 'error', text: 'Password is Required.', time: 2})
                            }else {
                                notie.alert({type: 'error', text: 'Something Went wrong', time: 2})
                            }
                        }
                    });
                }else{
                    $.ajax
                    ({
                        type: 'post',
                        url: '/register',
                        data: {
                            do_register: "do_register",
                            fname: fname,
                            lname: lname,
                            email: email,
                            phone: phone,
                            password: pass,
                            table_name: 'users',
                            account_type: account_type,
                            csrf: csrf_token,
                        },
                        success: function (response) {
                            document.getElementById("btn").innerHTML = "Register";
                            document.getElementById("btn").disabled = false;

                            response = $.parseJSON(response);
                            if (response['statusCode'] == "200") {
                                notie.alert({type: 'success', text: response['message'], time: 2})
                                location.href='/login'
                            } else if (response['statusCode'] == "202") {
                                notie.alert({type: 'error', text: response['message'], time: 2})

                            } else {
                                notie.alert({type: 'error', text: response['message'], time: 2})
                            }
                        }
                    });
                }

            }else {
                notie.alert({type: 'error', text: "Password Not Matched", time: 2})
            }
        }else{
            notie.alert({type: 'error', text: "Missing Required Input", time: 2})
        }
        return false;
    }
</script>
