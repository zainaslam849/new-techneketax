                    <!--begin::Logo-->
                    <a href="/" class="mb-10 mb-lg-20">
                        <img alt="Logo" src="{{ env.APP_URL }}assets/techneketax.png" class="h-40px" />
                    </a>
                    <!--end::Logo-->
                    <!--begin::Nav-->
                    <div class="stepper-nav">
                        {% if firm_id == "" and invite == "" %}
