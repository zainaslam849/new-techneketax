    <script>
        $(document).ready(function(){
            fetch_profile()
            fetch_social_key_data()
        });
        function fetch_profile() {
            var id = {{ loginId }};
            $.ajax({
                url: "/admin/fetch_profile",
                type: "POST",
                data: {
                    edit: 1,
                    id: id,
                },
                cache: false,
                success: function (dataResult) {
                    var dataResult = JSON.parse(dataResult);
                    console.log(dataResult);
                    $('#fullNameFirst').html(dataResult['users'][0].fname+' '+dataResult['users'][0].lname);
                    $('#fullName').html(dataResult['users'][0].fname+' '+dataResult['users'][0].lname);
                    $("#view_doc").html('');
                    var profileImage = dataResult['users'][0].profile_image;
                    var imageUrl = profileImage !== 'avatar.png'
                        ? "{{ env.APP_URL }}uploads/profile/" + profileImage
                        : "{{ env.APP_URL }}assets/dash/assets/media/svg/avatars/blank.svg";

                    // Set the background image dynamically
                    $('.image-input').css('background-image', 'url(' + imageUrl + ')');
                    $("#view_doc").html('');
                    if(dataResult['users'][0].profile_image != 'null' && dataResult['users'][0].profile_image != ''&& dataResult['users'][0].profile_image != null) {
                        $("#view_doc").append('<img src="{{ env.APP_URL }}uploads/profile/'+dataResult['users'][0].profile_image+'" >');
                        console.log(dataResult['users'][0].profile_image)
                    }else{
                        $("#view_doc").append('<img src="{{ env.APP_URL }}uploads/profile/avatar.png" >');
                        console.log("lora lele")
                    }
                    if(dataResult['appointment'] != ''){
                        $('#appointmentCount').html(dataResult['appointment']);
                    }else{
                        $('#appointmentCount').html(0);
                    }
                    if(dataResult['invoiceUnpaid'] != '') {
                        $('#invoiceUnpaidCount').html(dataResult['invoiceUnpaid']);
                    }else{
                        $('#invoiceUnpaidCount').html(0);
                    }
                    if(dataResult['invoicePaid'] != '') {
                        $('#invoicePaid').html(dataResult['invoicePaid']);
                    }else{
                        $('#invoicePaid').html(0);
                    }
                }
            });
        }
        function fetch_social_key_data() {
            var id = {{ loginId }};
            $.ajax({
                url: "/admin/fetch_social_key_data",
                type: "POST",
                data: {
                    edit: 1,
                },
                cache: false,
                success: function (dataResult) {
                    var dataResult = JSON.parse(dataResult);
                    console.log(dataResult);
                    $('#facebook_app_id').val(dataResult[0].facebook_app_id);
                    $('#facebook_app_secret').val(dataResult[0].facebook_app_secret);
                    $('#google_client_id').val(dataResult[0].google_client_id);
                    $('#google_client_secret').val(dataResult[0].google_client_secret);
                    $('#microsoft_client_id').val(dataResult[0].microsoft_client_id);
                    $('#microsoft_client_secret').val(dataResult[0].microsoft_client_secret);
                    $('#apple_client_id').val(dataResult[0].apple_client_id);
                    $('#apple_team_id').val(dataResult[0].apple_team_id);
                    $('#apple_key_id').val(dataResult[0].apple_key_id);
                }
            });
        }
        function twofaStatus(userID, status) {

            if(status == 0){
                var currStatus= "off";
            }else{
                var currStatus= "on";
            }
            var el = this;
            $(this).html('<i class="fa fa-circle-o-notch fa-spin"></i> loading...');
            var csrf_token = $("input[name=csrf]").val();
            $.ajax({
                url: '/admin/api',
                type: 'GET', data: {
                    id: userID,
                    status: currStatus,
                    page_name: "userStatusUpdate",
                    table_name:"users",
                    status_table:"twofa_status",
                    csrf: csrf_token
                }, success: function (data) {
                    fetch_profile()
                    if (data == 1) {
                        toastr.success('2FA Updated.');
                        $('#row').load(window.location.href + ' #row');
                        fetch_profile()
                    } else {
                        toastr.error('Something went wrong.');

                    }
                }

            });
        }
        function facebook_update() {
            document.getElementById('btn_facebook_update').disabled = true;
            var formData = new FormData(document.getElementById("FacebookKeyForm"));
            $.ajax({
                url: "/admin/profile/social-login-key",
                type: "POST",
                data: formData,
