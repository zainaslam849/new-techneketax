    <script src="https://code.jquery.com/jquery-3.7.1.min.js" integrity="sha256-/JqT3SQfawRcv/BIHPThkBvs0OEvtFFmqPF/lYI/Cxo=" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/perfect-scrollbar@1.5.3/dist/perfect-scrollbar.min.js"></script>

    <script>
        $(document).ready(function () {
            const groupId = $('#groupId').val();
            const userProfileImage = "{{ userProfileImage }}"
                ? "{{ env.APP_URL }}uploads/profile/{{ userProfileImage }}"
                : "https://avatar.iran.liara.run/username?username={{ loginName }}";
            //const wsUrl = 'wss://dev.techneketax.com/ws/';
            const wsUrl = 'ws://localhost:8000/';
            let ws;

            function initializeWebSocket() {
                try {
                    ws = new WebSocket(wsUrl);

                    ws.onopen = function () {
                        console.log('WebSocket connected');
                        if (groupId) {
                            getGroupMessages(groupId);
                        }
                    };

                    ws.onmessage = function (event) {
                        console.log('WebSocket message received:', event.data);
                        handleIncomingMessage(JSON.parse(event.data));
                    };

                    ws.onerror = function (error) {
                        console.error('WebSocket error:', error);
                    };

                    ws.onclose = function (event) {
                        console.warn('WebSocket connection closed:', event);
                    };
                } catch (error) {
                    console.error('Error initializing WebSocket:', error);
                }
            }

            initializeWebSocket();

            function getGroupMessages(groupId) {
                $.ajax({
                    url: `/chat/messages/group/${groupId}`,
                    method: 'GET',
                    dataType: 'json',
                    success: function (response) {
                        const chatContainer = $('#kt_chat_messenger_body [data-kt-element="messages"]');
                        chatContainer.empty(); // Clear existing messages

                        if (response.messages.length === 0) {
                            const noMessagesElement = `
                        <div class="text-muted text-center py-10">No messages</div>
                    `;
                            chatContainer.append(noMessagesElement);
                        } else {
                            let currentDate = '';

                            response.messages.forEach(message => {
                                const messageDate = timeAgo(message.timestamp);
                                if (messageDate !== currentDate) {
                                    currentDate = messageDate;
                                    const dateElement = `<div class="conversation-start"><span>${currentDate}</span></div>`;
                                    chatContainer.append(dateElement);
                                }

                                const messageElement = (message.sender === 'me') ?
                                    createOutgoingMessageElement(message) :
                                    createIncomingMessageElement(message);

                                chatContainer.append(messageElement);
                            });

                            chatContainer.scrollTop(chatContainer[0].scrollHeight);
                        }
                    },
                    error: function (xhr, status, error) {
                        console.error('AJAX error fetching messages:', xhr.responseText);
                    }
                });
            }

            function timeAgo(timestamp) {
                const now = new Date();
                const messageTime = new Date(timestamp);
                const diffInSeconds = Math.floor((now - messageTime) / 1000);

                const intervals = {
                    year: 31536000,
                    month: 2592000,
                    day: 86400,
                    hour: 3600,
                    minute: 60,
                    second: 1
                };

                for (const interval in intervals) {
                    const diff = Math.floor(diffInSeconds / intervals[interval]);
                    if (diff > 1) {
                        return `${diff} ${interval}s ago`;
                    } else if (diff === 1) {
                        return `1 ${interval} ago`;
                    }
                }
                return "just now";
            }

            function createOutgoingMessageElement(message) {
                return `
            <div class="d-flex justify-content-end mb-10">
                <div class="d-flex flex-column align-items-end">
                    <div class="d-flex align-items-center mb-2">
                        <div class="me-3">
                            <span class="text-muted fs-7 mb-1">${timeAgo(message.timestamp)}</span>
                            <a href="#" class="fs-5 fw-bolder text-gray-900 text-hover-primary ms-1">You</a>
                        </div>
                        <div class="symbol symbol-35px symbol-circle">
                            <img alt="Pic" src="${userProfileImage}" />
                        </div>
                    </div>
                    <div class="p-5 rounded bg-light-primary text-dark fw-bold mw-lg-400px text-end" data-kt-element="message-text">${message.message}</div>
                </div>
            </div>
        `;
            }

            function createIncomingMessageElement(message) {
                return `
            <div class="d-flex justify-content-start mb-10">
                <div class="d-flex flex-column align-items-start">
                    <div class="d-flex align-items-center mb-2">
                        <div class="symbol symbol-35px symbol-circle">
                            <img alt="Pic" src="${message.senderProfileImage}" />
                        </div>
                        <div class="ms-3">
                            <a href="#" class="fs-5 fw-bolder text-gray-900 text-hover-primary me-1">${message.senderName}</a>
                            <span class="text-muted fs-7 mb-1">${new Date(message.timestamp).toLocaleTimeString()}</span>
                        </div>
                    </div>
                    <div class="p-5 rounded bg-light-info text-dark fw-bold mw-lg-400px text-start" data-kt-element="message-text">${message.message}</div>
                </div>
            </div>
        `;
            }

            function handleIncomingMessage(message) {
                const chatContainer = $('#kt_chat_messenger_body [data-kt-element="messages"]');
                const messageElement = createIncomingMessageElement(message);
                chatContainer.append(messageElement);
                chatContainer.scrollTop(chatContainer[0].scrollHeight);
            }

            function sendMessage(groupId) {
                const chatInput = $('[data-kt-element="input"]');
                const chatMessageValue = chatInput.val().trim();
                if (chatMessageValue === '') return;

                const messageData = {
                    sender_id: '{{ loginId }}',
                    group_id: groupId,
                    message: chatMessageValue
                };

                try {
                    ws.send(JSON.stringify(messageData));
                    const messageElement = createOutgoingMessageElement({
                        message: chatMessageValue,
                        timestamp: new Date(),
                        senderProfileImage: userProfileImage
                    });
                    const chatContainer = $('#kt_chat_messenger_body [data-kt-element="messages"]');
                    chatContainer.append(messageElement);
                    chatContainer.scrollTop(chatContainer[0].scrollHeight);
                    chatInput.val(''); // Clear the input
                } catch (error) {
                    console.error('Error sending message through WebSocket:', error);
                }
            }

            $('[data-kt-element="input"]').on('keydown', function (event) {
                if (event.key === 'Enter') {
                    event.preventDefault();
                    sendMessage(groupId);
                }
            });

            $('[data-kt-element="send"]').on('click', function () {
                sendMessage(groupId);
            });
        });

        function timeAgo(timestamp) {
            const now = new Date();
            const messageTime = new Date(timestamp);
            const diffInSeconds = Math.floor((now - messageTime) / 1000);

            const intervals = {
                year: 31536000,
                month: 2592000,
                day: 86400,
                hour: 3600,
                minute: 60,
                second: 1
            };

            for (const interval in intervals) {
                const diff = Math.floor(diffInSeconds / intervals[interval]);
                if (diff > 1) {
                    return `${diff} ${interval}s ago`;
                } else if (diff === 1) {
                    return `1 ${interval} ago`;
                }
            }
            return "just now";
        }

    </script>
    <script>
        //SEARCH
        $(document).ready(function () {
            $('input[name="search"]').on('keyup', function () {
                const searchValue = $(this).val().toLowerCase().trim();
                console.log("Search Value:", searchValue);
                if (searchValue === "") {
                    $('#kt_chat_contacts_body .d-flex.flex-stack').show();
                    return;
                }
                $('#kt_chat_contacts_body .d-flex.flex-stack').each(function () {
                    const userName = $(this).attr('data-user-name').toLowerCase();
                    const userEmail = $(this).attr('data-user-email').toLowerCase();
                    if (userName.includes(searchValue) || userEmail.includes(searchValue)) {
                        $(this).show();
                    } else {
                        $(this).hide();
                    }
                });
            });
        });


    </script>


